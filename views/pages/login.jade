    body.external-page.sb-l-c.sb-r-c
        div#main.animated.fadeIn
            div#content_wrapper
                div#canvas-wrapper
                    canvas#demo-canvas
                section#content
                    div.admin-form.theme-info.mw500#login
                        div.row.table-layout
                            a(href="test")
                                img.center-block.img-responsive(src="https://www.cellsmartpos.com/assets/img/v1/logo.png" style="max-width: 275px;")
                        div.panel.mt30.mb25
                            form#contact(method="post" action="/login")
                                div.panel-body.bg-light.p25.pb15
                                    div.section
                                        label.field-label.text-muted.fs18.mb10(for="email") Email
                                        label.field.prepend-icon(for="email")
                                            input.gui-input(name="email" required type="text" for='email' placeholder="Enter email address")
                                            label.field-icon(for="email")
                                                i.fa.fa-user

                                    div.section
                                        label.field-label.text-muted.fs18.mb10(for="username") Password
                                        label.field.prepend-icon(for="password")
                                            input.gui-input(name="password" required type="password" for=password placeholder="Enter password")
                                            label.field-icon(for="password")
                                                i.fa.fa-lock
                                div.panel-footer.clearfix
                                    button.button.btn-primary.mr10.pull-right#login(type="submit") Sign In
                                    label.switch.ib.switch-primary.mt10
                            
                        div.alert.alert-primary.alert
                            strong What New!

                        div.alert.alert-success.alert.alert-border-left(style="margin-bottom: 0")
                            span Time of sign up now you can get customer cell phone number
                        div.alert.alert-alert.alert.alert-border-left(style="margin-bottom: 0")
                            span Search box if you enter csp id. that will open store page.
                        div.alert.alert-system.alert.alert-border-left(style="margin-bottom: 0")
                            span you can hide commercial for any store from store dashbaord
                        
                    
                                    
                                    
    include ../endScript.html

    script.

        CanvasBG.init({
          Loc: {
            x: window.innerWidth / 2,
            y: window.innerHeight / 3.3
          },
        });

        if('#{failedLogin}' != ''){
            //console.log('#{failedLogin}');
            showNotify("Login Failed",'#{failedLogin}','error');
        }
