doctype html
html
  head
    title CSP Billing
    <meta charset="utf-8">
    <meta name="keywords" content="HTML5 Bootstrap 3 Admin Template UI Theme" />
    <meta name="description" content="AdminDesigns - A Responsive HTML5 Admin UI Framework">
    <meta name="author" content="AdminDesigns">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">



    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css'>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css">

    <link rel='stylesheet' type='text/css' href='https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700'>

    link(rel='stylesheet', href='/fonts/icomoon/icomoon.css')

    <!-- Datatables CSS -->
    <link rel="stylesheet" type="text/css" href="/plugins/css/dataTables.bootstrap.css">

    <!-- Datatables Addons CSS -->
    <link rel="stylesheet" type="text/css" href="/plugins/css/dataTables.plugins.css">

    <!-- Favicon -->
    <link rel="shortcut icon" href="/img/cellSmartIcon.ico">

    <link rel="stylesheet" type="text/css" href="/plugins/css/magnific-popup.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/theme.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/admin-forms.min.css">

    <script src="/plugins/js/moment.min.js"></script>

    <script src="/jquery/jquery-1.11.1.min.js"></script>
    <script src="/plugins/js/jquery.maskedinput.min.js"></script>
    <script src="/jquery/jquery.validate.min.js"></script>
